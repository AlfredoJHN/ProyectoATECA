@model IEnumerable<ProyectoATECA.Models.Ficha>
@{
    ViewBag.Title = "Atendiendo";

}

<div class="row">

    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/Img/ccss-logo.png" alt="ATECA">
                <div class="carousel-caption">
                    <h5 style="color:black; font:885px;">Bienvenido al Sistema de Gestion de filas</h5>
                    <p style="color:black; font:555px;">Caja Costarricense del Seguro Social</p>
                </div>
            </div>

            @foreach (var item in Model)
            {
                <div class="item">
                    <img src="~/Img/ccss-logo.png" alt="Fichas">
                    <div class="carousel-caption">
                        <h5 style="color:black; font:885px;">
                            Atendiendo a:
                        </h5>
                        <h5 style="color:black; font:555px;">
                            Ficha: @Html.DisplayFor(modelItem => item.codigoFicha)
                        </h5>
                        <p style="color:black; font:555px;">
                            Caja: @Html.DisplayFor(modelItem => item.Servicio.nombre)
                        </p>
                    </div>
                </div>
            }
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

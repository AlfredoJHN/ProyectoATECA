<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>https://atecaweb.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>atecaweb.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>ATECAWEB</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <InstallAspNetCoreSiteExtension>False</InstallAspNetCoreSiteExtension>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$ATECAWEB</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ATECA_BDEntities" Order="1" Enabled="False">
          <Destination Path="Data Source=ateca.database.windows.net;Initial Catalog=ATECA_BD;User ID=ATECA;Password=ALFREDO_123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="ProyectoATECA.Models.ATECA_BDEntities, ProyectoATECA" MigrationConfiguration="ProyectoATECA.Migrations.Configuration, ProyectoATECA" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="ATECA_BDEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/Models.Model.csdl|res://*/Models.Model.ssdl|res://*/Models.Model.msl;provider=System.Data.SqlClient;provider connection string="Data Source=ateca.database.windows.net;Initial Catalog=ATECA_BD;User ID=ATECA;Password=ALFREDO_123"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>